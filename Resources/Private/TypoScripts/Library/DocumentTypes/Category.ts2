prototype(TYPO3.Neos:PrimaryContent).SimplyAdmireBlogCategory {
	condition = ${q(node).is('[instanceof SimplyAdmire.Blog:Category]')}
	type = 'SimplyAdmire.Blog:Category'
	@position = 'start'
}

prototype(SimplyAdmire.Blog:Category) {
	templatePath = 'resource://SimplyAdmire.Blog/Private/Templates/DocumentTypes/Category.html'
}