prototype(TYPO3.Neos:PrimaryContent).SimplyAdmireBlogAuthor {
	condition = ${q(node).is('[instanceof SimplyAdmire.Blog:Author]')}
	type = 'SimplyAdmire.Blog:Author'
	@position = 'start'
}

prototype(SimplyAdmire.Blog:Author) {
	templatePath = 'resource://SimplyAdmire.Blog/Private/Templates/DocumentTypes/Author.html'
}