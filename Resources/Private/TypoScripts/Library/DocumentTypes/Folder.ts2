prototype(TYPO3.Neos:PrimaryContent).SimplyAdmireBlogFolder {
	condition = ${q(node).is('[instanceof SimplyAdmire.Blog:Folder]')}
	type = 'SimplyAdmire.Blog:Folder'
	@position = 'start'
}

prototype(SimplyAdmire.Blog:Folder) {
	templatePath = 'resource://SimplyAdmire.Blog/Private/Templates/DocumentTypes/Folder.html'
}